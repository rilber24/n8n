    {
      "parameters": {
        "language": "python",
        "pythonCode": "# === ParametrizaciÃ³n ===\nJASPER_HOST = 'https://jasper.envibol.site:8443'\nJASPER_PATH = '/jasperserver/rest_v2/reports/Reports/ENVIBOL/RRHH/Asistencia.pdf'\nJASPER_USER = 'envibol'\nJASPER_PASS = '123456789'\n# Rango de fechas\nFECHA_INI = '2025-08-01'\nFECHA_FIN = '2025-08-31'\nFECHA_LEIBLE = '08/2025'\nDEFAULT_CC = '591'\n\nfrom urllib.parse import quote\nimport unicodedata\n\ndef only_digits(s: str) -> str:\n    return ''.join(ch for ch in str(s or '') if ch.isdigit())\n\ndef quitar_tildes_mayus(texto: str) -> str:\n    if not texto:\n        return ''\n    t = unicodedata.normalize('NFD', texto)\n    t = ''.join(ch for ch in t if unicodedata.category(ch) != 'Mn')\n    t = t.replace('Ã±','n').replace('Ã‘','N')\n    return ' '.join(t.upper().split())\n\nitems_in = _input.all()\nif not items_in:\n    return []\n\nsalida = []\nfor it in items_in:\n    j = it.json or {}\n\n    # 1) Nombre completo desde cualquiera de las variantes disponibles\n    nombre = j.get('fullName')\n    if not nombre:\n        nombre = ' '.join([j.get('nombre',''), j.get('apellidoPaterno',''), j.get('apellidoMaterno','')]).strip()\n        nombre = ' '.join(nombre.split())\n    first_name = (nombre.split()[0] if nombre else '').upper()\n\n    # 2) areaName si viene desde upstream; si no, por defecto\n    area_name = j.get('areaName') or 'AREA DE GESTION Y PLANIFICACION'\n\n    # 3) NÃºmero normalizado\n    number = j.get('number') or j.get('celular') or j.get('telefono') or ''\n    number = only_digits(number)\n    if number.startswith(('6','7')) and not number.startswith(DEFAULT_CC):\n        number = DEFAULT_CC + number\n    # Si tu Evolution necesita @c.us, descomenta:\n    # number = number + '@c.us'\n\n    # 4) nombre_apellido param para Jasper (MAYÃšSCULAS, sin tildes, Ã‘->N, URL-encoded)\n    nombre_apellido = j.get('nombreApellidoParam')\n    if not nombre_apellido:\n        base = quitar_tildes_mayus(nombre)\n        nombre_apellido = quote(base, safe='')\n\n    # 5) Link Jasper\n    link = (\n        f\"{JASPER_HOST}{JASPER_PATH}\"\n        f\"?j_username={JASPER_USER}&j_password={JASPER_PASS}\"\n        f\"&fecha_inicio={FECHA_INI}&fecha_fin={FECHA_FIN}\"\n        f\"&nombre_apellido={nombre_apellido}\"\n    )\n\n    # 6) Mensaje\n    texto = (\n        f\"Â¡Hola, {first_name}! ðŸ‘‹\\n\"\n        f\"Buenos dÃ­as y un excelente inicio de jornada. En ENVIBOL, valoramos mucho tu compromiso en el Ã¡rea de {area_name}. ðŸ’ª\\n\"\n        f\"Â¡A darlo todo hoy! ðŸš€ Estamos seguros de que lograremos grandes cosas juntos. âœ¨\\n\\n\"\n        f\"ðŸ“„ Reporte de Asistencia ({FECHA_LEIBLE})\\n\"\n        f\"ðŸ”— Descargar: {link}\"\n    )\n\n    # 7) Nombre de archivo\n    safe_name = quitar_tildes_mayus(nombre).replace(' ', '_') or 'ASISTENCIA'\n    file_name = f\"{safe_name}_Asistencia_{FECHA_INI}_{FECHA_FIN}.pdf\"\n\n    salida.append({\n        'json': {\n            'fullName': nombre,\n            'firstName': first_name,\n            'areaName': area_name,\n            'number': number,\n            'texto': texto,\n            'link': link,\n            'fileName': file_name\n        }\n    })\n\nreturn salida"
      },
