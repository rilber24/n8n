    {
      "parameters": {
        "jsCode": "// Devuelve UN item con json.payload = array limpio y vÃ¡lido\nconst items = $input.all()\n  .map(i => i.json || {})\n  .filter(j =>\n    (j.trade_type === 'BUY' || j.trade_type === 'SELL') &&\n    Number.isFinite(Number(j.price_bob)) &&\n    Number.isFinite(Number(j.available_usdt)) &&\n    j.captured_at\n  )\n  .map(j => ({\n    captured_at: j.captured_at,\n    trade_type: j.trade_type, // BUY / SELL\n    advertiser_name: (j.advertiser_name || '').trim(),\n    price_bob: Number(j.price_bob),\n    available_usdt: Number(j.available_usdt),\n  }));\n\nreturn [{ json: { payload: items } }];"
      },
