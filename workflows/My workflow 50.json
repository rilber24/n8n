    {
      "parameters": {
        "operation": "executeQuery",
        "query": "INSERT INTO p2p.p2p_offers_core\n  (captured_at, trade_type, advertiser_name, price_bob, available_usdt)\nVALUES\n  ($1::timestamptz,\n   $2::p2p.trade_type_enum,\n   $3::text,\n   $4::numeric,\n   $5::numeric)\nON CONFLICT (captured_minute, trade_type, advertiser_name)\nDO UPDATE SET\n  price_bob      = EXCLUDED.price_bob,\n  available_usdt = EXCLUDED.available_usdt,\n  captured_at    = GREATEST(p2p.p2p_offers_core.captured_at, EXCLUDED.captured_at);",
        "options": {
          "queryReplacement": ""
        }
      },
